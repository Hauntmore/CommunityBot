{
  "compilerOptions": {
    "target": "esNext", 
    "module": "ESNext", 
    "resolveJsonModule": true,
    "outDir": "dist",
    "strict": true, 
    "strictNullChecks": false,
    "moduleResolution": "node",
    "allowSyntheticDefaultImports": true,
    "esModuleInterop": true,
    "skipLibCheck": true,
    "forceConsistentCasingInFileNames": true 
  }
}
